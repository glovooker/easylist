@page
@model WebApp___EasyList.Pages.AuditLogModel
@{
    ViewData["Title"] = "Action Log";
}
<script src="~/js/auditlog.js"></script>
<div class="bs-docs-section d-flex flex-column">
    <br />
    <div class="d-flex flex-row justify-content-between">
        <h3 class="display-7 mb-3">Audit Log</h3>
    </div>
    <div class="row">
        <table id="tblLogAction" class="display mt-5" style="width:100%">
            <thead>
            <th>ID</th>
            <th>Action Date & Time</th>
            <th>Action Type</th>
            <th>Affected Object ID</th>
            <th>Affected Table</th>
            </thead>
        </table>
        <br />
    </div>
</div>