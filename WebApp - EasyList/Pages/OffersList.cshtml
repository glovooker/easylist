@page
@model WebApp___EasyList.Pages.OffersListModel
@{
    ViewData["Title"] = "Offers List";
}
<script src="~/js/offerslist.js"></script>
<div class="bs-docs-section d-flex flex-column">
    <br />
    <div class="d-flex flex-row justify-content-between">
        <h3 class="display-7 mb-3">Offers List</h3>
        <div>
            <button id="btnBack" class="btn btn-warning w-auto mx-4" type="button" tabindex="0">Back</button>
        </div>
    </div>
    <div class="row" id="tblContainer">
        <table id="tblOffer" class="display mt-5" style="width:100%">
            <thead>
            <th>ID</th>
            <th>Offerer ID</th>
            <th>Tender ID</th>
            <th>Chosen</th>
            <th>Total Cost</th>
            <th>Deliver Date</th>
            </thead>
        </table>
        <br />
    </div>

</div>